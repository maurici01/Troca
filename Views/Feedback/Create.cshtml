@model Troca.Models.Feedback

@{
    ViewData["Title"] = "Create";
}



<h4>Reclame-aqui</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Chat" class="control-label"></label>
                <input asp-for="Chat" class="form-control" />
                <span asp-validation-for="Chat" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Enviar" class="btn btn-success" />
                 <input  style="background-color:#070743" type="submit" value="Voltar" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>



